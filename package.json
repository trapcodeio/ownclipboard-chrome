{
  "name": "ownclipboard-chrome",
  "version": "0.1.1",
  "private": true,
  "source": "package/background.html",
  "scripts": {
    "start": "parcel",
    "dev": "vite --port 4111",
    "build": "node build-package.js && vite build",
    "tsc:build": "node build-package.js && npx vue-tsc --noEmit && vite build",
    "tsc:chrome": "npx tsc --noEmit -p tsconfig.chrome.json",
    "tsc:vue": "npx tsc --noEmit",
    "serve": "vite preview"
  },
  "dependencies": {
    "@types/chrome": "^0.0.193",
    "@vitejs/plugin-vue": "^3.0.1",
    "lodash": "^4.17.21",
    "object-collection": "^3.0.1",
    "revue-components": "^1.3.22",
    "timeago.js": "^4.0.2",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0-0",
    "vue-tsc": "^0.39.0",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.0",
    "@tailwindcss/forms": "^0.5.2",
    "@trapcode/browser-storage": "^2.4.2",
    "@types/node": "^18.6.0",
    "@vue/compiler-sfc": "^3.0.0",
    "autoprefixer": "^9",
    "axios": "^0.21.0",
    "esbuild": "^0.14.49",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "sass": "^1.26.5",
    "tailwindcss": "^3.1.6",
    "typescript": "^4.7.4",
    "vite": "^3.0.2"
  }
}
